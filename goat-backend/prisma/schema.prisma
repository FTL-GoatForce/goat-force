// This is your Prisma schema file,


generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// DEAL SCHEMA
model Deal{
  id Int @id @default(autoincrement())
  name String

}

